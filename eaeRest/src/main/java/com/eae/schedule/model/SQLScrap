SELECT t1.GUID, t1.CREATED_DATE, t1.DATE, t1.UPDATED_DATE, t1.SERVICE_PERIOD_ID, t0.GUID, t0.CREATED_DATE, t0.ENDS, t0.STARTS, t0.UPDATED_DATE, t0.SERVICEDAY_GUID FROM SERVICE_DAY t1 LEFT OUTER JOIN SHIFT t0 ON (t0.SERVICEDAY_GUID = t1.GUID) WHERE (t1.SERVICE_PERIOD_ID = '6d61904d-d4e3-435d-8ebc-57c42e1a315c')
SELECT t1.GUID, t1.CREATED_DATE, t1.DATE, t1.UPDATED_DATE, t1.SERVICE_PERIOD_ID, t0.GUID, t0.CREATED_DATE, t0.ENDS, t0.STARTS, t0.UPDATED_DATE, t0.SERVICEDAY_GUID FROM SERVICE_DAY t1 LEFT OUTER JOIN (SERVICE_DAY_SHIFT t3 JOIN SHIFT t0 ON (t0.GUID = t3.shifts_GUID)) ON (t3.ServiceDay_GUID = t1.GUID) LEFT OUTER JOIN SERVICE_PERIOD t2 ON (t2.GUID = t1.SERVICE_PERIOD_ID) WHERE (t1.SERVICE_PERIOD_ID = '6d61904d-d4e3-435d-8ebc-57c42e1a315c') ORDER BY t1.DATE ASC
SELECT t1.GUID, t1.CREATED_DATE, t1.DATE, t1.UPDATED_DATE, t1.SERVICE_PERIOD_ID, t0.GUID, t0.CREATED_DATE, t0.ENDS, t0.STARTS, t0.UPDATED_DATE, t0.SERVICEDAY_GUID FROM SERVICE_DAY t1 LEFT OUTER JOIN SHIFT t0 ON (t0.SERVICEDAY_GUID = t1.GUID) LEFT OUTER JOIN SERVICE_PERIOD t2 ON (t2.GUID = t1.SERVICE_PERIOD_ID) WHERE (t1.SERVICE_PERIOD_ID = '6d61904d-d4e3-435d-8ebc-57c42e1a315c') ORDER BY t1.DATE ASC