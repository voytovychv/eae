{
  "_version": "1.1.0",
  "sap.app": {
	"_version": "1.1.0",
	"id": "org.eae.tools",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"_version": "1.1.0",
	"technology": "UI5",
	"deviceTypes": {
	  "desktop": true,
	  "tablet": true,
	  "phone": true
	},
	"supportedThemes": [
		"sap_hcb",
		"sap_belize"
	]
  },
  "sap.ui5": {
	"_version": "1.1.0",
	"rootView": {
		"viewName": "org.eae.tools.view.App",
		"id": "aeaAppView",
		"type": "XML"
	}, 
	"dependencies": {
	  "minUI5Version": "1.48",
	  "libs": {
		"sap.m": {},
		"sap.uxap" : {}
	  }
	},
	"contentDensities": {
		"compact": true,
		"cozy": true
	},	
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "org.eae.tools.i18n.i18n"
		}
	  }
	},
	"resources": {
	  "css": [
		{
		  
		}
	  ]
	},
	
	"routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"viewType": "XML",
		"viewPath": "org.eae.tools.view",
		"controlId": "eaeApp",
		"controlAggregation": "pages",
        "bypassed": {
           "target": "notFound"
        }
	  },
	  "routes": [
		{
		  "pattern": "",
		  "name": "landingPage",
		  "target": "landing"
		},
		{
			"pattern" : "overview",
			"name" : "overviewPeriods",
			"target" : "overview"
		},
		{
			"pattern" : "overview/{periodId}",
			"name" : "overviewPeriod",
			"target" : "overviewPeriod"
		},
		{
			"pattern" : "overviewPublishersAccounts",
			"name" : "overviewPublishers",
			"target" : "overviewPublishers"
			
		},
		{
			"pattern" : "cartPlaces",
			"name" : "cartPlaceWorklist",
			"target" : "cartPlaceWorklist"
			
		},
		{
			"pattern" : "calendar/{publisherId}/{periodId}",
			"name" : "publisherCalendar",
			"target" : "publisherCalendar"
		},
		{
			"pattern" : "schedule/{scheduleId}/period/{periodId}",
			"name" : "overviewSchedule",
			"target" : "overviewSchedule"
		},
		{
			"pattern" : "cartSchedule/{scheduleId}/period/{periodId}",
			"name" : "cartSchedule",
			"target" : "cartSchedule"
		},
		{
			"pattern" : "schedules",
			"name" : "scheduleWorklist",
			"target" : "scheduleWorklist"
		},
		{
			"pattern" : "login",
			"name" : "login",
			"target" : "login"
		},
		{
			"pattern" : "publisherProfile/{publisherId}/:isMyAccount:",
			"name" : "publisherProfile",
			"target" : "publisherProfile"
		},
		{
			"pattern" : "myShifts/{publisherId}",
			"name" : "myShifts",
			"target" : "myShifts"
		}

	  ],
	  "targets": {
		"landing": {
		  "viewName": "Landing",
		  "viewLevel" : 1
		},
		"overview" :{
		  "viewName" : "PeriodsWorlkist",
		  "viewLevel" : 2
		},
		"overviewPeriod" : {
		  "viewName" : "PeriodOverview",
		  "viewLevel" : 3
		},
		"overviewPublishers" : {
		  "viewName" : "PublishersWorklist",
		  "viewLevel" : 2
		},
		"publisherCalendar" : {
		  "viewName" : "PublisherCalendar",
		  "viewLevel" : 2
		},
		"overviewSchedule" : {
		  "viewName" : "ScheduleOverview",
		  "viewLevel" : 2
		},
		"cartPlaceWorklist" : {
		  "viewName" : "CartPlaceWorklist",
		  "viewLevel" : 2
		},
		"scheduleWorklist" : {
		  "viewName" : "ScheduleWorklist",
		  "viewLevel" : 2
		},	
		"cartSchedule" : {
		  "viewName" : "CartSchedule",
		  "viewLevel" : 2
		},
		"login" : {
		  "viewName" : "Login",
		  "viewLevel" : 2
		},
		"notFound" : {
			"viewName" : "NotFound",
			"transition": "show"
		},
		"publisherProfile" : {
		  "viewName" : "PublisherProfile",
		  "viewLevel" : 2
		
		},
		"myShifts" : {
		  "viewName" : "MyShifts",
		  "viewLevel" : 2
		
		}
	  }
	}
  }
}
